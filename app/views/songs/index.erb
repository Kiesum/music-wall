<div>
<h1>This is a list of songs</h1>
</div>

<% @songs.each do |song| %>
  <div class="song">
    <h3><a href="/songs/<%= song.id %>"><%= song.song_title %></a> <small>(by: <%= song.author %>)</small> <a href="<%= song.url %>"><small>(url: <%= song.url %>)</small></a></h3>
  </div>
<% end %>
  <div>
  <a href="/songs/new">Post a message... It's FREE (for now)!</a>
  </div>
